{
  "name": "mf/quick-order",
  "description": "MF Quick Order",
  "version": "1.0.0",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "extra": {
    "shopware-plugin-class": "MF\\QuickOrder\\MFQuickOrder",
    "author": "Mikail Fidan",
    "label": {
      "en-GB": "Adds a quick order page."
    }
  },
  "require": {
    "php": ">= 7.2",
    "shopware/core": "6.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.3.4",
    "phpstan/phpstan": "^0.12.18",
    "shopware/core": "*",
    "shopware/storefront": "*"
  },
  "autoload": {
    "psr-4": {
      "MF\\QuickOrder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MF\\QuickOrder\\Tests\\": "tests/"
    }
  }
}
